package example2;

import arrayextension.ArrayData;

public class main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ArrayData<String> data = new ArrayData<String>();
		data.map((item) -> {
			String cad = item.toString();
			cad = cad + " transformado ";
			return item;
		});
	}

}
